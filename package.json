    "elevation:probe": "node scripts/elevation_rate_limit_test.js"
  },
  "devDependencies": {
    "@types/three": "^0.182.0",
    "typescript": "^5.6.3",
    "vite": "^5.2.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "fflate": "^0.8.2",
    "onnxruntime-web": "^1.23.2",
    "three": "^0.182.0"
  }
}
