{
  "name": "visopti",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "trainer": "vite --open /trainer.html?trainer=1",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "ml:convert:yolo": "node ml/scripts/convert_visopti_zip_to_yolo.js <zipPath> <outDir>",
